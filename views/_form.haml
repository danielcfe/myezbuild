- for input in @options
    %label{:for => "#{input[:name]}"}
    %input.form-control{type: "#{input[:html_type]}", name: "#{input[:name]}", id: "#{input[:name]}", value: "#{@lead.send(input[:name])}", placeholder: "#{input[:placeholder]}", title: "error"}
%button.btn.btn-success{type: "submit", style:"width:100%;margin-top: 1.2em;margin-bottom: 20px;"} SEND
